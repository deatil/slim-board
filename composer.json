{
    "name": "deatil/slim-board",
    "description": "A slim-board",
    "keywords": [
        "microframework",
        "rest",
        "router",
        "psr7"
    ],
    "homepage": "http://github.com/deatil/slim-board",
    "license": "MIT",
    "authors": [
        {
            "name": "deatil",
            "email": "deatil@github.com",
            "homepage": "https://github.com/deatil",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "bryanjhv/slim-session": "~4.0",
        "catfan/medoo": "^2.1",
        "gregwar/captcha": "^1.1",
        "inhere/php-validate": "^2.8",
        "monolog/monolog": "^2.7",
        "nesbot/carbon": "^2.59",
        "php-di/php-di": "^6.4",
        "ramsey/uuid": "^4.3",
        "slim/psr7": "^1.5",
        "slim/slim": "^4.10",
        "slim/twig-view": "^3.3",
        "vlucas/phpdotenv": "^5.4.1",
        "zeuxisoo/slim-whoops": "0.7.*"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true,
        "allow-plugins": {}
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Skg\\Board\\": "skg/board/"
        },
        "files": [
            "skg/board/Support/helper.php"
        ]
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}
